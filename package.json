  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "npx crlf --set=LF node_modules/.bin/webpack-bundle-diff",
    "build": "lerna run build --stream",
    "postbuild": "cpy packages/readme/dist/README.md packages/react-fontawesome-svg-icon/ & cpy packages/readme/dist/README.md ./",
    "test": "lerna run test --stream",
    "lint": "lerna run lint --stream",
    "version:update": "lerna version --no-push",
    "syncpack:mismatches": "syncpack list-mismatches"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "chalk": "^5.0.0",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "eslint": "^8.6.0",
    "eslint-plugin-react": "^7.28.0",
    "filesize": "^8.0.6",
    "fs-extra": "^10.0.0",
    "gzip-size": "^7.0.0",
    "lerna": "^4.0.0",
    "react-dev-utils": "^12.0.0",
    "rollup": "^2.63.0",
    "syncpack": "^6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eugenezinovyev/react-fontawesome-svg-icon.git"
  }
}
