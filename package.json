    "css-minimizer-webpack-plugin": "^3.3.1",
    "eslint": "^8.7.0",
    "eslint-plugin-react": "^7.28.0",
    "fs-extra": "^10.0.0",
    "gzip-size": "^7.0.0",
    "promptly": "^3.2.0",
    "syncpack": "^6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eugenezinovyev/react-fontawesome-svg-icon.git"
