    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.34",
    "jest": "^27.3.1",
    "react": "^17.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
