{
  "name": "docs",
  "version": "1.0.3",
  "main": "index.js",
  "author": "Eugene Zinovyev <eugene.zinovyev@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "copy:apps": "cpy ../apps/baseline/dist/ dist/baseline && cpy ../apps/comparison/dist/ dist/comparison",
    "prestats:diff:wbd": "wbd diff dist/baseline/stats.json dist/comparison/stats.json -o dist/stats-diff.json",
    "stats:diff:wbd": "wbd report dist/stats-diff.json -o dist/stats-diff.md",
    "stats:diff:bundle-stats": "bundle-stats --html --json dist/comparison/stats.json dist/baseline/stats.json",
    "build:diff": "yarn copy:apps && (yarn stats:diff:wbd & yarn stats:diff:bundle-stats)",
    "build:readme:merge": "concat -o dist/README.md src/badges.md src/disclaimer.md src/motivation.md src/how.md src/installation.md src/features.md src/bundle-comparison.md dist/stats-diff.md",
    "build:readme:copy": "cpy dist/README.md ../react-fontawesome-svg-icon & cpy dist/README.md ../..",
    "build:readme": "yarn build:readme:merge && yarn build:readme:copy",
    "build": "yarn build:diff && yarn build:readme"
  },
  "devDependencies": {
    "baseline": "^1.0.2",
    "bundle-stats": "^3.2.0",
    "comparison": "^1.0.3",
    "concat": "^1.0.3",
    "cpy-cli": "^3.1.1",
    "webpack-bundle-diff": "^1.1.0"
  }
}
